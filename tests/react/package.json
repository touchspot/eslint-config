{
	"name": "test-react",
	"private": true,
	"type": "module",
	"imports": {
		"#pkg/*.js": "./src/*.ts",
		"#pkg/*.jsx": "./src/*.tsx"
	},
	"scripts": {
		"check:format": "prettier --check .",
		"check:type": "tsc --noEmit",
		"fix:format": "prettier --write --log-level=warn .",
		"inspect": "config-inspector",
		"test": "eslint ."
	},
	"prettier": "@touchspot/prettier-config",
	"devDependencies": {
		"@touchspot/eslint-config": "workspace:^",
		"@types/react": "^19.2.2",
		"jotai": "^2.15.1"
	}
}
