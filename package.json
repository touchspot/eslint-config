{
	"name": "@touchspot/eslint-config",
	"version": "2.0.0",
	"description": "TOUCHSPOT ESLint config",
	"keywords": [
		"eslint",
		"typescript"
	],
	"homepage": "https://github.com/touchspot/eslint-config/tree/main",
	"bugs": {
		"url": "https://github.com/touchspot/eslint-config/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/touchspot/eslint-config.git"
	},
	"license": "MIT",
	"author": "mozisan <mozisan@users.noreply.github.com>",
	"type": "module",
	"exports": {
		".": "./dist/lib/lib.js",
		"./presets/*": "./dist/lib/presets/*.js",
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"package.json",
		"README.md"
	],
	"scripts": {
		"build": "tspc --project .",
		"check:format": "prettier --check .",
		"check:lint": "eslint .",
		"check:type": "tsc --noEmit",
		"check:unused": "knip",
		"fix:format": "prettier --write --log-level=warn .",
		"fix:lint": "eslint --fix .",
		"inspect": "config-inspector",
		"prepare": "husky"
	},
	"prettier": "@touchspot/prettier-config",
	"dependencies": {
		"@eslint/js": "^9.30.1",
		"@next/eslint-plugin-next": "^15.3.5",
		"eslint-config-prettier": "^10.1.5",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-better-tailwindcss": "^3.5.2",
		"eslint-plugin-check-file": "^3.3.0",
		"eslint-plugin-functional": "^9.0.2",
		"eslint-plugin-import-x": "^4.16.1",
		"eslint-plugin-no-relative-import-paths": "^1.6.1",
		"eslint-plugin-perfectionist": "^4.15.0",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "0.0.0-experimental-60b5271a-20250709",
		"eslint-plugin-unicorn": "^59.0.1",
		"eslint-plugin-unused-imports": "^4.1.4",
		"globals": "^16.3.0",
		"typescript-eslint": "^8.36.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.8.1",
		"@commitlint/config-conventional": "^19.8.1",
		"@commitlint/config-pnpm-scopes": "^19.8.1",
		"@commitlint/cz-commitlint": "^19.8.1",
		"@eslint/config-inspector": "^1.1.0",
		"@touchspot/prettier-config": "^2.0.0",
		"@tsconfig/next": "^2.0.3",
		"@tsconfig/node22": "^22.0.2",
		"@tsconfig/strictest": "^2.0.5",
		"@types/node": "^22.16.3",
		"@types/react": "^19.1.8",
		"better-typescript-lib": "^2.11.0",
		"commitizen": "^4.3.1",
		"eslint": "^9.30.1",
		"husky": "^9.1.7",
		"knip": "^5.61.3",
		"lint-staged": "^16.1.2",
		"next": "^15.3.5",
		"prettier": "^3.6.2",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"tailwindcss": "^4.1.11",
		"ts-patch": "^3.3.0",
		"turbo": "^2.5.4",
		"typescript": "^5.8.3",
		"typescript-transform-paths": "^3.5.5"
	},
	"peerDependencies": {
		"eslint": "^9",
		"next": "^15",
		"react": "^19",
		"react-dom": "^19",
		"tailwindcss": "^4"
	},
	"peerDependenciesMeta": {
		"next": {
			"optional": true
		},
		"react": {
			"optional": true
		},
		"react-dom": {
			"optional": true
		},
		"tailwindcss": {
			"optional": true
		}
	},
	"packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad",
	"publishConfig": {
		"access": "public"
	}
}
