{
	"name": "@touchspot/eslint-config",
	"version": "3.0.5",
	"description": "TOUCHSPOT ESLint config",
	"keywords": [
		"eslint",
		"typescript"
	],
	"homepage": "https://github.com/touchspot/eslint-config/tree/main",
	"bugs": {
		"url": "https://github.com/touchspot/eslint-config/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/touchspot/eslint-config.git"
	},
	"license": "MIT",
	"author": "mozisan <mozisan@users.noreply.github.com>",
	"type": "module",
	"imports": {
		"#pkg/*.js": "./dist/src/*.js"
	},
	"exports": {
		".": "./dist/lib/lib.js",
		"./addons/*": "./dist/lib/addons/*.js",
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"package.json",
		"README.md"
	],
	"scripts": {
		"build": "tsc",
		"check:format": "prettier --check .",
		"check:lint": "eslint .",
		"check:type": "tsc --noEmit",
		"check:unused": "knip",
		"fix:format": "prettier --write --log-level=warn .",
		"fix:lint": "eslint --fix .",
		"inspect": "config-inspector",
		"prepare": "husky"
	},
	"prettier": "@touchspot/prettier-config",
	"dependencies": {
		"@eslint/js": "^9.39.1",
		"@next/eslint-plugin-next": "^16.0.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-better-tailwindcss": "^3.7.10",
		"eslint-plugin-check-file": "^3.3.1",
		"eslint-plugin-functional": "^9.0.2",
		"eslint-plugin-import-x": "^4.16.1",
		"eslint-plugin-no-relative-import-paths": "^1.6.1",
		"eslint-plugin-perfectionist": "^4.15.1",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "7.0.1",
		"eslint-plugin-unicorn": "^62.0.0",
		"eslint-plugin-unused-imports": "^4.3.0",
		"globals": "^16.5.0",
		"typescript-eslint": "^8.46.3"
	},
	"devDependencies": {
		"@commitlint/cli": "^20.1.0",
		"@commitlint/config-conventional": "^20.0.0",
		"@commitlint/config-pnpm-scopes": "^20.1.0",
		"@eslint/config-inspector": "^1.3.0",
		"@eslint/core": "^0.17.0",
		"@touchspot/prettier-config": "^3.0.3",
		"@tsconfig/next": "^2.0.3",
		"@tsconfig/node22": "^22.0.2",
		"@tsconfig/strictest": "^2.0.7",
		"@types/node": "^22.19.0",
		"@types/react": "^19.2.2",
		"better-typescript-lib": "^2.12.0",
		"commitizen": "^4.3.1",
		"cz-git": "^1.12.0",
		"eslint": "^9.39.1",
		"husky": "^9.1.7",
		"knip": "^5.67.1",
		"lint-staged": "^16.2.6",
		"next": "^16.0.1",
		"prettier": "^3.6.2",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"tailwindcss": "^4.1.16",
		"turbo": "^2.6.0",
		"typescript": "^5.9.3",
		"typescript-transform-paths": "^3.5.5"
	},
	"peerDependencies": {
		"eslint": "^9",
		"next": "^15 || ^16",
		"react": "^19",
		"react-dom": "^19",
		"tailwindcss": "^3 || ^4"
	},
	"peerDependenciesMeta": {
		"next": {
			"optional": true
		},
		"react": {
			"optional": true
		},
		"react-dom": {
			"optional": true
		},
		"tailwindcss": {
			"optional": true
		}
	},
	"packageManager": "pnpm@10.18.3+sha512.bbd16e6d7286fd7e01f6b3c0b3c932cda2965c06a908328f74663f10a9aea51f1129eea615134bf992831b009eabe167ecb7008b597f40ff9bc75946aadfb08d",
	"publishConfig": {
		"access": "public"
	}
}
