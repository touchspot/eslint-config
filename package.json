{
	"name": "@touchspot/eslint-config",
	"version": "0.0.0",
	"description": "TOUCHSPOT ESLint config",
	"keywords": [
		"eslint",
		"typescript"
	],
	"homepage": "https://github.com/touchspot/eslint-config/tree/main",
	"bugs": {
		"url": "https://github.com/touchspot/eslint-config/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/touchspot/eslint-config.git"
	},
	"license": "MIT",
	"author": "mozisan <mozisan@users.noreply.github.com>",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/lib/lib.d.ts",
			"import": "./dist/lib/lib.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"package.json",
		"README.md"
	],
	"scripts": {
		"prepare": "husky",
		"build": "tspc --project tsconfig.build.json",
		"check:format": "prettier --check .",
		"check:lint": "eslint .",
		"check:type": "tsc --noEmit",
		"check:unused": "knip",
		"fix:format": "prettier --write --log-level=warn .",
		"fix:lint": "eslint --fix .",
		"inspect": "config-inspector"
	},
	"dependencies": {
		"@eslint/js": "^9.9.1",
		"@next/eslint-plugin-next": "canary",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-check-file": "^2.8.0",
		"eslint-plugin-functional": "^7.0.2",
		"eslint-plugin-import-x": "^4.0.0",
		"eslint-plugin-no-relative-import-paths": "^1.5.5",
		"eslint-plugin-perfectionist": "^3.2.0",
		"eslint-plugin-react": "^7.35.0",
		"eslint-plugin-react-hooks": "next",
		"eslint-plugin-tailwindcss": "^3.17.4",
		"eslint-plugin-unicorn": "^55.0.0",
		"eslint-plugin-unused-imports": "^4.1.3",
		"globals": "^15.9.0",
		"typescript-eslint": "^8.3.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.4.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@commitlint/config-pnpm-scopes": "^19.1.0",
		"@commitlint/cz-commitlint": "^19.4.0",
		"@eslint/config-inspector": "^0.5.4",
		"@touchspot/prettier-config": "^1.0.0",
		"@tsconfig/next": "^2.0.3",
		"@tsconfig/node20": "^20.1.4",
		"@tsconfig/strictest": "^2.0.5",
		"@types/eslint-config-prettier": "^6.11.3",
		"@types/eslint__js": "^8.42.3",
		"@types/node": "^22.5.0",
		"@types/react": "^18.3.4",
		"@typescript-eslint/utils": "^8.3.0",
		"better-typescript-lib": "^2.8.0",
		"commitizen": "^4.3.0",
		"eslint": "^9.9.1",
		"husky": "^9.1.5",
		"inquirer": "^9.3.6",
		"knip": "^5.27.4",
		"lint-staged": "^15.2.9",
		"next": "^14.2.6",
		"prettier": "^3.3.3",
		"react": "^18.3.1",
		"tailwindcss": "^3.4.10",
		"ts-patch": "^3.2.1",
		"turbo": "^2.0.14",
		"typescript": "^5.5.4",
		"typescript-transform-paths": "^3.5.0"
	},
	"peerDependencies": {
		"eslint": "^9",
		"next": "^14.2.6",
		"react": "^18.3.1",
		"tailwindcss": "^3.4.10"
	},
	"peerDependenciesMeta": {
		"next": {
			"optional": true
		},
		"react": {
			"optional": true
		},
		"tailwindcss": {
			"optional": true
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"packageManager": "pnpm@9.9.0"
}
